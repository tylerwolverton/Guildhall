StateMachine
{
    Number attackDamage;

    Number otherId;
    Number otherId2;
    OnEvent( EnterCollision )
    {
        otherId2 = otherId;
        if( otherId2 != -1 )
        {
            FireEvent( DamageEntity( id = otherId, damage = attackDamage ) );
            FireEvent( AddScreenShake( intensity = .05 ) );
        }

        FireEvent( DestroySelf() );
    }
}